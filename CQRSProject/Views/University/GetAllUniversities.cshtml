@model List<GetAllUniversityQueryResult>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br>
    <table>
        <tr>
            <th>#</th>
            <th>University</th>
            <th>City</th>
            <th>Delete</th>
            <th>Update</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.UniversityID</td>
                <td>@item.Name</td>
                <td>@item.Town</td>
                <td><a href="/University/DeleteUniversity/@item.UniversityID" class="btn btn-outline-danger">Delete</a></td>
                <td><a href="/University/UpdateUniversity/@item.UniversityID" class="btn btn-outline-success">Update</a></td>
            </tr>

        }

    </table>
    <a href="/University/AddUniversity" class="btn btn-outline-primary">New University</a>
</div>
